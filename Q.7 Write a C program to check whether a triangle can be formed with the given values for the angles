#include <stdio.h>

int main() {
  int angle1, angle2, angle3;
  int sum;

  printf("Enter First angle of a triangle: \n");
  scanf("%d", &angle1);

  printf("Enter Second angle of a triangle: \n");
  scanf("%d", &angle2);

  printf("Enter Third angle of a triangle: \n");
  scanf("%d", &angle3);

  sum = angle1 + angle2 + angle3;

  if (sum == 180) {
    printf("A triangle can be formed.\n");
  } else {
    printf("A triangle cannot be formed.\n");
  }

  return 0;
}
